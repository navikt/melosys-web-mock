{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/vurdering-schema.json",
  "type": "object",
  "title": "The Root Schema",
  "additionalProperties": false,
  "required": [
    "behandlingID",
    "vurdering"
  ],
  "properties": {
    "behandlingID": {
      "$id": "#/properties/behandlingID",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/behandlingID"
    },
    "vurdering": {
      "$id": "#/properties/vurdering",
      "type": "object",
      "title": "The Vurdering Schema",
      "additionalProperties": false,
      "required": [
        "lovvalgsbestemmelser",
        "feilmeldinger"
      ],
      "properties": {
        "lovvalgsbestemmelser": {
          "$id": "#/properties/vurdering/properties/lovvalgsbestemmelser",
          "type": "array",
          "title": "The Lovvalgsbestemmelser Schema",
          "uniqueItems": true,
          "items": {
            "$id": "#/properties/vurdering/properties/lovvalgsbestemmelser/items",
            "type": "object",
            "title": "The Items Schema",
            "additionalProperties": false,
            "required": [
              "artikkel",
              "betingelser"
            ],
            "properties": {
              "artikkel": {
                "$id": "#/properties/vurdering/properties/lovvalgsbestemmelser/items/properties/artikkel",
                "type": "string",
                "title": "The Artikkel Schema",
                "default": "",
                "examples": [
                  "12.1"
                ],
                "pattern": "^(.*)$"
              },
              "betingelser": {
                "$id": "#/properties/vurdering/properties/lovvalgsbestemmelser/items/properties/betingelser",
                "type": "array",
                "title": "The Betingelser Schema",
                "uniqueItems": true,
                "items": {
                  "$id": "#/properties/vurdering/properties/lovvalgsbestemmelser/items/properties/betingelser/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "additionalProperties": false,
                  "required": [
                    "argument",
                    "krav",
                    "resultat"
                  ],
                  "properties": {
                    "argument": {
                      "$id": "#/properties/vurdering/properties/lovvalgsbestemmelser/items/properties/betingelser/items/properties/argument",
                      "type": "string",
                      "title": "The Argument Schema",
                      "default": "",
                      "examples": [
                        "Arbeidsgiver har virksomhet i landet arbeidstakeren sendes fra"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "krav": {
                      "$id": "#/properties/vurdering/properties/lovvalgsbestemmelser/items/properties/betingelser/items/properties/krav",
                      "type": "string",
                      "title": "The Krav Schema",
                      "default": "",
                      "examples": [
                        "er sann"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "resultat": {
                      "$id": "#/properties/vurdering/properties/lovvalgsbestemmelser/items/properties/betingelser/items/properties/resultat",
                      "type": "string",
                      "title": "The Resultat Schema",
                      "default": "",
                      "examples": [
                        "OPPFYLT"
                      ],
                      "pattern": "^(.*)$"
                    }
                  }
                }
              }
            }
          }
        },
        "feilmeldinger": {
          "$id": "#/properties/vurdering/properties/feilmeldinger",
          "type": "array",
          "title": "The Feilmeldinger Schema",
          "uniqueItems": true,
          "items": {
            "$id": "#/properties/vurdering/properties/feilmeldinger/items",
            "type": "object",
            "title": "The Items Schema",
            "additionalProperties": false,
            "required": [
              "kategori",
              "melding"
            ],
            "properties": {
              "kategori": {
                "$id": "#/properties/vurdering/properties/feilmeldinger/items/properties/kategori",
                "type": "object",
                "title": "The Kategori Schema",
                "additionalProperties": false,
                "required": [
                  "alvorlighetsgrad",
                  "beskrivelse"
                ],
                "properties": {
                  "alvorlighetsgrad": {
                    "$id": "#/properties/vurdering/properties/feilmeldinger/items/properties/kategori/properties/alvorlighetsgrad",
                    "type": "string",
                    "title": "The Alvorlighetsgrad Schema",
                    "default": "",
                    "examples": [
                      "FEIL"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "beskrivelse": {
                    "$id": "#/properties/vurdering/properties/feilmeldinger/items/properties/kategori/properties/beskrivelse",
                    "type": "string",
                    "title": "The Beskrivelse Schema",
                    "default": "",
                    "examples": [
                      "Det er implementert delvis maskinell støtte for denne forespørselen."
                    ],
                    "pattern": "^(.*)$"
                  }
                }
              },
              "melding": {
                "$id": "#/properties/vurdering/properties/feilmeldinger/items/properties/melding",
                "type": "string",
                "title": "The Melding Schema",
                "default": "",
                "examples": [
                  "Søkeren kan ikke arbeide på både skip og sokkel."
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    }
  }
}
