{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/sok-fagsaker-schema.json",
  "type": "array",
  "title": "The Root Schema",
  "uniqueItems": true,
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "additionalProperties": false,
    "required": [
      "saksnummer",
      "sakstype",
      "behandlingstype",
      "behandlingsstatus",
      "opprettetDato",
      "soknadsperiode",
      "land"
    ],
    "properties": {
      "saksnummer": {
        "$id": "#/items/properties/saksnummer",
        "type": "string",
        "title": "The Saksnummer Schema",
        "default": "",
        "examples": [
          "4"
        ],
        "pattern": "^(.*)$"
      },
      "sakstype": {
        "$id": "#/items/properties/sakstype",
        "title": "The Sakstype Schema",
        "$ref": "http://melosys.nav.no/definitions-schema.json#/definitions/kodeverk"
      },
      "behandlingstype": {
        "$id": "#/items/properties/behandlingstype",
        "title": "The Behandlingstype Schema",
        "$ref": "http://melosys.nav.no/definitions-schema.json#/definitions/kodeverk"
      },
      "behandlingsstatus": {
        "$id": "#/items/properties/behandlingsstatus",
        "type": "object",
        "title": "The Behandlingsstatus Schema",
        "$ref": "http://melosys.nav.no/definitions-schema.json#/definitions/kodeverk"
      },
      "opprettetDato": {
        "$id": "#/items/properties/opprettetDato",
        "type": "string",
        "format": "date-time",
        "title": "The Opprettetdato Schema",
        "default": "",
        "examples": [
          "2017-10-19T12:31:36.123Z"
        ],
        "pattern": "^(.*)$"
      },
      "soknadsperiode": {
        "$id": "#/items/properties/soknadsperiode",
        "type": "object",
        "title": "The Soknadsperiode Schema",
        "additionalProperties": false,
        "required": [
          "fom",
          "tom"
        ],
        "properties": {
          "fom": {
            "$id": "#/items/properties/soknadsperiode/properties/fom",
            "type": "string",
            "format": "date",
            "title": "The Fom Schema",
            "default": "",
            "examples": [
              "2018-08-01"
            ],
            "pattern": "^(.*)$"
          },
          "tom": {
            "$id": "#/items/properties/soknadsperiode/properties/tom",
            "type": "string",
            "format": "date",
            "title": "The Tom Schema",
            "default": "",
            "examples": [
              "2018-12-31"
            ],
            "pattern": "^(.*)$"
          }
        }
      },
      "land": {
        "$id": "#/items/properties/land",
        "type": "array",
        "title": "The Land Schema",
        "uniqueItems": true,
        "items": {
          "$id": "#/items/properties/land/items",
          "type": "string",
          "title": "The Items Schema",
          "default": "",
          "examples": [
            "GB"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}
