{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/oppgaver-sok-schema.json",
  "type": ["array","null"],
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "$ref": "#/definitions/saksbehandling"
  },
  "definitions": {
    "saksbehandling": {
      "type": ["object","null"],
      "additionalProperties": false,
      "required": [
        "oppgaveID",
        "redigerbart",
        "sammensattNavn",
        "saksnummer",
        "sakstype",
        "behandling",
        "aktivTil",
        "soknadsperiode",
        "prioritet",
        "land",
        "versjon",
        "ansvarligID"
      ],
      "properties": {
        "oppgaveID": {
          "$id": "#/properties/saksbehandling/items/properties/oppgaveID",
          "type": "string",
          "title": "The Oppgaveid Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "174562068"
          ],
          "pattern": "^(.*)$"
        },
        "redigerbart": {
          "$id": "#/properties/saksbehandling/items/properties/redigerbart",
          "title": "Er oppgaven redigerbart",
          "type": "boolean",
          "default": true,
          "examples": [true, false]
        },
        "sammensattNavn": {
          "$id": "#/properties/saksbehandling/items/properties/sammensattNavn",
          "type": "string",
          "title": "The Sammensattnavn Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "GLITRENDE HATT"
          ],
          "pattern": "^(.*)$"
        },
        "saksnummer": {
          "$id": "#/properties/saksbehandling/items/properties/saksnummer",
          "type": "string",
          "title": "The Saksnummer Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "4"
          ],
          "pattern": "^(.*)$"
        },
        "sakstype": {
          "$id": "#/properties/saksbehandling/items/properties/sakstype",
          "title": "The Sakstypekode Schema",
          "description": "An explanation about the purpose of this instance.",
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/kodeverk"
        },
        "behandling": {
          "$id": "#/properties/saksbehandling/items/properties/behandling",
          "type": "object",
          "title": "The Behandling Schema",
          "description": "An explanation about the purpose of this instance.",
          "additionalProperties": false,
          "required": [
            "behandlingID",
            "behandlingstype",
            "behandlingsstatus",
            "sisteOpplysningerHentetDato",
            "erUnderOppdatering"
          ],
          "properties": {
            "behandlingID": {
              "$id": "#/properties/saksbehandling/items/properties/behandling/properties/behandlingID",
              "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/behandlingID"
            },
            "behandlingstype": {
              "$id": "#/properties/saksbehandling/items/properties/behandling/properties/behandlingstype",
              "title": "The behandlingstype Schema",
              "description": "An explanation about the purpose of this instance.",
              "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/kodeverk"
            },
            "behandlingsstatus": {
              "$id": "#/properties/saksbehandling/items/properties/behandling/properties/behandlingsstatus",
              "title": "The behandlingsstatus Schema",
              "description": "An explanation about the purpose of this instance.",
              "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/kodeverk"
            },
            "sisteOpplysningerHentetDato": {
              "title": "The sisteOpplysningerHentetDato Schema",
              "$id": "#/properties/saksbehandling/items/properties/behandling/properties/sisteOpplysningerHentetDato",
              "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dato-tid"
            },
            "erUnderOppdatering": {
              "$id": "#/properties/saksbehandling/items/properties/behandling/properties/erUnderOppdatering",
              "type": "boolean",
              "title": "The erUnderOppdatering Schema",
              "description": "An explanation about the purpose of this instance.",
              "default": "",
              "examples": [
                false,
                true
              ]
            }
          }
        },
        "aktivTil": {
          "$id": "#/properties/saksbehandling/items/properties/aktivTil",
          "type": "string",
          "format": "date",
          "title": "The Aktivtil Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "2018-01-28"
          ]
        },
        "soknadsperiode": {
          "title": "The Soknadsperiode Schema",
          "description": "An explanation about the purpose of this instance.",
          "$id": "#/properties/saksbehandling/items/properties/soknadsperiode",
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/nullable-periode"
        },
        "prioritet": {
          "$id": "#/properties/saksbehandling/items/properties/prioritet",
          "type": "string",
          "title": "The Prioritet Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "HOY"
          ],
          "pattern": "^(.*)$"
        },
        "land": {
          "$id": "#/properties/saksbehandling/items/properties/land",
          "type": "array",
          "title": "The Land Schema",
          "description": "An explanation about the purpose of this instance.",
          "uniqueItems": true,
          "additionalItems": false,
          "items": {
            "$id": "",
            "type": "string",
            "title": "The LandKode item Schema",
            "default": "",
            "examples": [
              "NO"
            ],
            "pattern": "^(.*)$"
          }
        },
        "versjon": {
          "$id": "#/properties/saksbehandling/items/properties/versjon",
          "type": "integer",
          "title": "The Versjon Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": 0,
          "examples": [
            1
          ]
        },
        "ansvarligID": {
          "$id": "#/properties/saksbehandling/items/properties/ansvarligID",
          "type": "string",
          "title": "The Ansvarligid Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "Z991001"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}

