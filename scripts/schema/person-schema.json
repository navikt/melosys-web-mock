{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/person-schema.json",
  "type": "object",
  "title": "The Root Schema",
  "additionalProperties": false,
  "required": [
    "fnr",
    "sivilstand",
    "statsborgerskap",
    "sammensattNavn",
    "personstatus",
    "kjoenn",
    "foedselsdato"
  ],
  "properties": {
    "fnr": {
      "$id": "#/properties/fnr",
      "type": "string",
      "title": "The Fnr Schema",
      "default": "",
      "examples": [
        "19117220349"
      ],
      "pattern": "^(.*)$"
    },
    "sivilstand": {
      "$id": "#/properties/sivilstand",
      "type": "string",
      "title": "The Sivilstand Schema",
      "default": "",
      "examples": [
        "Registrert partner"
      ],
      "pattern": "^(.*)$"
    },
    "statsborgerskap": {
      "$id": "#/properties/statsborgerskap",
      "type": "string",
      "title": "The Statsborgerskap Schema",
      "default": "",
      "examples": [
        "NOR"
      ],
      "pattern": "^(.*)$"
    },
    "sammensattNavn": {
      "$id": "#/properties/sammensattNavn",
      "type": "string",
      "title": "The Sammensattnavn Schema",
      "default": "",
      "examples": [
        "HATT GLITRENDE"
      ],
      "pattern": "^(.*)$"
    },
    "personstatus": {
      "$id": "#/properties/personstatus",
      "type": "string",
      "title": "The Personstatus Schema",
      "default": "",
      "examples": [
        "Bosatt"
      ],
      "pattern": "^(.*)$"
    },
    "kjoenn": {
      "$id": "#/properties/kjoenn",
      "type": "string",
      "title": "The Kjoenn Schema",
      "default": "",
      "examples": [
        "M","K"
      ],
      "pattern": "^(.*)$"
    },
    "foedselsdato": {
      "$id": "#/properties/foedselsdato",
      "type": "string",
      "format": "date",
      "title": "The Foedselsdato Schema",
      "default": "",
      "examples": [
        "1972-11-19"
      ]
    }
  }
}
