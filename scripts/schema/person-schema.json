{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/person-schema.json",
  "type": "object",
  "title": "The Root Schema",
  "additionalProperties": false,
  "required": [
    "fnr",
    "sivilstand",
    "statsborgerskap",
    "sammensattNavn",
    "personstatus",
    "kjoenn",
    "foedselsdato",
    "bostedsadresse"
  ],
  "properties": {
    "fnr": {
      "$id": "#/properties/fnr",
      "type": "string",
      "title": "The Fnr Schema",
      "default": "",
      "examples": [
        "19117220349"
      ],
      "pattern": "^(.*)$"
    },
    "sivilstand": {
      "$id": "#/properties/sivilstand",
      "title": "The Sivilstand Schema",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/kodeverk"
    },
    "statsborgerskap": {
      "$id": "#/properties/statsborgerskap",
      "title": "The Statsborgerskap Schema",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/kodeverk"
    },
    "sammensattNavn": {
      "$id": "#/properties/sammensattNavn",
      "type": "string",
      "title": "The Sammensattnavn Schema",
      "default": "",
      "examples": [
        "HATT GLITRENDE"
      ],
      "pattern": "^(.*)$"
    },
    "personstatus": {
      "$id": "#/properties/personstatus",
      "title": "The Personstatus Schema",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/kodeverk"
    },
    "kjoenn": {
      "$id": "#/properties/kjoenn",
      "title": "The Kjoenn Schema",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/kodeverk"
    },
    "foedselsdato": {
      "title": "The Foedselsdato Schema",
      "$id": "#/properties/foedselsdato",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dato"
    },
    "bostedsadresse": {
      "$id": "#/properties/bostedsadresse",
      "type": "object",
      "title": "The Bostedsadresse Schema",
      "additionalProperties": false,
      "required": [
        "gateadresse",
        "postnr",
        "poststed",
        "land"
      ],
      "properties": {
        "gateadresse": {
          "$id": "#/properties/gateadresse",
          "type": "object",
          "title": "The Gateadresse Schema",
          "additionalProperties": false,
          "required": [
            "gatenavn",
            "gatenummer",
            "husnummer",
            "husbokstav"
          ],
          "properties": {
            "gatenavn": {
              "$id": "#/properties/gateadresse/properties/gatenavn",
              "type": ["string","null"],
              "title": "The Gatenavn Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "gatenummer": {
              "$id": "#/properties/bostedsadresse/properties/gateadresse/properties/gatenummer",
              "type": ["integer","null"],
              "title": "The Gatenummer Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "husnummer": {
              "$id": "#/properties/bostedsadresse/properties/gateadresse/properties/husnummer",
              "type": ["integer","null"],
              "title": "The Husnummer Schema",
              "default": null,
              "examples": [
                null,
                "1"
              ]
            },
            "husbokstav": {
              "$id": "#/properties/bostedsadresse/properties/gateadresse/properties/husbokstav",
              "type": ["string","null"],
              "title": "The Husbokstav Schema",
              "default": null,
              "examples": [
                null
              ]
            }
          }
        },
        "postnr": {
          "$id": "#/properties/bostedsadresse/properties/postnr",
          "type": ["string","null"],
          "title": "The Postnr Schema",
          "default": null,
          "examples": [
            null
          ]
        },
        "poststed": {
          "$id": "#/properties/bostedsadresse/properties/poststed",
          "type": ["string","null"],
          "title": "The poststed Schema",
          "default": null,
          "examples": [
            null
          ]
        },
        "land": {
          "title": "The Land Schema",
          "$id": "#/properties/bostedsadresse/properties/land",
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/kodeverk"
        }
      }
    }
  }
}
