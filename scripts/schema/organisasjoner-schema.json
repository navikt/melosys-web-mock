      "default": "",
      "examples": [
        "910099035"
      ],
      "pattern": "^(.*)$"
    },
    "oppstartdato": {
      "$id": "#/properties/oppstartdato",
      "title": "The Oppstartdato Schema",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/nullable-dato"
    },
    "postadresse": {
