      "type": [
        "string",
        "null"
      ],
      "title": "The Organisasjonsform Schema",
      "default": null,
      "examples": [
        null
      ]
    },
    "navn": {
      "$id": "#/properties/navn",
      "type": "string",
      "title": "The Navn Schema",
      "default": "",
      "examples": [
        "SKARSVÃ…G OG VANSE"
      ],
      "pattern": "^(.*)$"
    },
    "forretningsadresse": {
      "$id": "#/properties/forretningsadresse",
      "title": "The Forretningsadresse Schema",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/strukturertadresse"
    }
  }
}
