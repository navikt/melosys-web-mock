{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/definitions-schema.json",
  "title": "The Definitions Schema",
  "definitions": {
    "kodeverk": {
      "type": "object",
      "title": "The Kodeverk Schema",
      "additionalProperties": false,
      "required": [
        "kode",
        "term"
      ],
      "properties": {
        "kode": {
          "type": "string",
          "title": "The Kode Schema",
          "default": "",
          "examples": [
            "OPPRETTET"
          ],
          "pattern": "^(.*)$"
        },
        "term": {
          "type": "string",
          "title": "The Term Schema",
          "default": "",
          "examples": [
            "Opprettet"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "behandlingID": {
      "$id": "#/properties/behandlingID",
      "type": ["integer"],
      "title": "The BehandlingID Schema",
      "default": 0,
      "examples": [
        1000023948
      ]
    },
    "aarmaaned": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}$",
      "examples": [
        "2016-12"
      ]
    },
    "dato":  {
      "type": "string",
      "format": "date",
      "examples": ["2018-08-27"]
    },
    "dato-tid": {
      "type": "string",
      "format": "date-time",
      "examples": [
        "2018-08-27T07:16:03.273Z"
      ]
    },
    "periode": {
      "title": "Periode med fom tom",
      "type": "object",
      "required": [
        "fom",
        "tom"
      ],
      "additionalProperties": false,
      "properties": {
        "fom": {
          "type": ["string"],
          "format": "date",
          "examples": ["2014-03-12"]
        },
        "tom": {
          "type": ["string","null"],
          "format": "date",
          "examples": ["2014-03-12", null]
        }
      }
    },
    "nullable-periode": {
      "title": "Periode med fom tom",
      "type": "object",
      "required": [
        "fom",
        "tom"
      ],
      "additionalProperties": false,
      "properties": {
        "fom": {
          "type": ["string", "null"],
          "format": "date",
          "examples": ["2014-03-12"]
        },
        "tom": {
          "type": ["string","null"],
          "format": "date",
          "examples": ["2014-03-12", null]
        }
      }
    },
    "nullable-dato":  {
      "type": ["string","null"],
      "format": "date",
      "examples": ["2018-08-27"]
    },
    "nullable-dato-tid": {
      "type": ["string","null"],
      "format": "date-time",
      "examples": [
        "2018-08-27T07:16:03.273Z", null
      ]
    },
    "adresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "adressetype"
      ],
      "if": {
        "properties": {
          "adressetype": {"const": "STRUKTURERT"}
        }
      },
      "then": {
        "required": ["strukturertAdresse"]
      },
      "else": {
        "required": ["ustrukturertAdresse"]
      },
      "properties": {
        "adressetype": {
          "type": "string",
          "enum": ["STRUKTURERT", "USTRUKTURERT"]
        },
        "strukturertAdresse": {
          "type": "object",
          "additionalProperties": false,
          "required": [
            "gatenavn",
            "husnr",
            "postnr",
            "poststed",
            "landKode"
          ],
          "properties": {
            "gatenavn": {
              "type": "string",
              "minLength": 1
            },
            "husnr": {
              "type": "string",
              "minLength": 1
            },
            "region": {
              "type": ["string", "null"],
              "minLength": 1
            },
            "postnr": {
              "type": "string",
              "minLength": 1
            },
            "poststed": {
              "type": "string",
              "minLength": 1
            },
            "landKode": {
              "type": "string",
              "minLength": 1
            }
          }
        },
        "ustrukturertAdresse": {
          "type": "object",
          "additionalProperties": false,
          "required": [
            "landKode"
          ],
          "properties": {
            "adresselinje1": {
              "type": "string",
              "minLength": 1
            },
            "adresselinje2": {
              "type": "string",
              "minLength": 1
            },
            "adresselinje3": {
              "type": "string",
              "minLength": 1
            },
            "adresselinje4": {
              "type": "string",
              "minLength": 1
            },
            "adresselinje5": {
              "type": "string",
              "minLength": 1
            },
            "landKode": {
              "type": "string",
              "minLength": 1
            }
          }
        }
      }
    }
  }
}
