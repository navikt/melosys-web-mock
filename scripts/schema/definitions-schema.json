{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/definitions-schema.json",
  "title": "The Definitions Schema",
  "definitions": {
    "kodeverk": {
      "type": "object",
      "title": "The Kodeverk Schema",
      "additionalProperties": false,
      "required": [
        "kode",
        "term"
      ],
      "properties": {
        "kode": {
          "type": "string",
          "title": "The Kode Schema",
          "default": "",
          "examples": [
            "OPPRETTET"
          ],
          "pattern": "^(.*)$"
        },
        "term": {
          "type": "string",
          "title": "The Term Schema",
          "default": "",
          "examples": [
            "Opprettet"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "behandlingID": {
      "$id": "#/properties/behandlingID",
      "type": ["integer"],
      "title": "The BehandlingID Schema",
      "default": 0,
      "examples": [
        1000023948
      ]
    },
    "aarmaaned": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}$",
      "examples": [
        "2016-12"
      ]
    },
    "dato":  {
      "type": "string",
      "format": "date",
      "examples": ["2018-08-27"]
    },
    "dato-tid": {
      "type": "string",
      "format": "date-time",
      "examples": [
        "2018-08-27T07:16:03.273Z"
      ]
    },
    "periode": {
      "title": "Periode med fom tom",
      "type": "object",
      "required": [
        "fom",
        "tom"
      ],
      "additionalProperties": false,
      "properties": {
        "fom": {
          "type": ["string"],
          "format": "date",
          "examples": ["2014-03-12"]
        },
        "tom": {
          "type": ["string","null"],
          "format": "date",
          "examples": ["2014-03-12", null]
        }
      }
    },
    "nullable-periode": {
      "title": "Periode med fom tom",
      "type": "object",
      "required": [
        "fom",
        "tom"
      ],
      "additionalProperties": false,
      "properties": {
        "fom": {
          "type": ["string", "null"],
          "format": "date",
          "examples": ["2014-03-12"]
        },
        "tom": {
          "type": ["string","null"],
          "format": "date",
          "examples": ["2014-03-12", null]
        }
      }
    },
    "nullable-dato":  {
      "type": ["string","null"],
      "format": "date",
      "examples": ["2018-08-27"]
    },
    "nullable-dato-tid": {
      "type": ["string","null"],
      "format": "date-time",
      "examples": [
        "2018-08-27T07:16:03.273Z", null
      ]
    },
    "adresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "adressetype"
      ],
      "oneOf": [
        {
          "properties": {
            "adressetype": {"const": "STRUKTURERT"}
          },
          "required": ["strukturertAdresse"]
        },
        {
          "properties": {
            "adressetype": {"const": "USTRUKTURERT"}
          },
          "required": ["ustrukturertAdresse"]
        }
      ],
      "properties": {
        "adressetype": {
          "type": "string",
          "enum": ["STRUKTURERT", "USTRUKTURERT"]
        },
        "strukturertAdresse": {
          "$ref": "#/definitions/strukturertAdresse"
        },
        "ustrukturertAdresse": {
          "$ref": "#/definitions/ustrukturertAdresse"
        }
      }
    },
    "strukturertAdresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "gatenavn",
        "postnummer",
        "poststed",
        "landKode"
      ],
      "properties": {
        "gatenavn": {
          "type": "string",
          "minLength": 1,
          "examples": [
            "Waldemar Thranes gate"
          ]
        },
        "husnummer": {
          "type": "string",
          "minLength": 1,
          "examples": [
            "98"
          ]
        },
        "region": {
          "type": "string",
          "minLength": 1,
          "examples": [
            "Bydel Sagene"
          ]
        },
        "postnummer": {
          "type": "string",
          "minLength": 1,
          "examples": [
            "0175"
          ]
        },
        "poststed": {
          "type": "string",
          "minLength": 1,
          "examples": [
            "OSLO"
          ]
        },
        "landKode": {
          "type": "string",
          "minLength": 1,
          "examples": [
            "NO"
          ]
        }
      }
    },
    "ustrukturertAdresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "landKode"
      ],
      "properties": {
        "adresselinjer": {
          "type": "array",
          "additionalItems": false,
          "maxItems": 5,
          "items": {
            "type": "string"
          }
        },
        "landKode": {
          "type": "string",
          "default": "",
          "examples": [
            "NO"
          ]
        }
      }
    }
  }
}
