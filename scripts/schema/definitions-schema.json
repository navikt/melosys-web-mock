{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/definitions-schema.json",
  "title": "The Definitions Schema",
  "definitions": {
    "kodeverk": {
      "type": ["object"],
      "title": "The Kodeverk Schema",
      "additionalProperties": false,
      "required": [
        "kode",
        "term"
      ],
      "properties": {
        "kode": {
          "type": "string",
          "title": "The Kode Schema",
          "default": "",
          "examples": [
            "OPPRETTET"
          ],
          "pattern": "^(.*)$"
        },
        "term": {
          "type": "string",
          "title": "The Term Schema",
          "default": "",
          "examples": [
            "Opprettet"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "behandlingID": {
      "$id": "#/properties/behandlingID",
      "type": ["integer"],
      "title": "The BehandlingID Schema",
      "default": 0,
      "examples": [
        1000023948
      ]
    },
    "aarmaaned": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}$",
      "examples": [
        "2016-12"
      ]
    },
    "dato":  {
      "type": "string",
      "format": "date",
      "examples": ["2018-08-27"]
    },
    "dato-tid": {
      "type": "string",
      "format": "date-time",
      "examples": [
        "2018-08-27T07:16:03.273Z"
      ]
    },
    "periode": {
      "title": "Periode med fom tom",
      "type": "object",
      "required": [
        "fom",
        "tom"
      ],
      "additionalProperties": false,
      "properties": {
        "fom": {
          "type": ["string"],
          "format": "date",
          "examples": ["2014-03-12"]
        },
        "tom": {
          "type": ["string","null"],
          "format": "date",
          "examples": ["2014-03-12", null]
        }
      }
    },
    "nullable-periode": {
      "title": "Periode med fom tom",
      "type": "object",
      "required": [
        "fom",
        "tom"
      ],
      "additionalProperties": false,
      "properties": {
        "fom": {
          "type": ["string", "null"],
          "format": "date",
          "examples": ["2014-03-12"]
        },
        "tom": {
          "type": ["string","null"],
          "format": "date",
          "examples": ["2014-03-12", null]
        }
      }
    },
    "nullable-dato":  {
      "type": ["string","null"],
      "format": "date",
      "examples": ["2018-08-27"]
    },
    "nullable-dato-tid": {
      "type": ["string","null"],
      "format": "date-time",
      "examples": [
        "2018-08-27T07:16:03.273Z", null
      ]
    },
    "strukturertadresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "gateadresse",
        "postnr",
        "poststed",
        "land"
      ],
      "properties": {
        "gateadresse": {
          "$id": "#/properties/strukturertadresse/properties/gateadresse",
          "type": "object",
          "title": "The Gateadresse Schema",
          "additionalProperties": false,
          "required": [
            "gatenummer",
            "husbokstav",
            "husnummer",
            "gatenavn"
          ],
          "properties": {
            "gatenummer": {
              "$id": "#/properties/strukturertadresse/properties/gateadresse/properties/gatenummer",
              "type": [
                "string",
                "null"
              ],
              "title": "The Gatenummer Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "husbokstav": {
              "$id": "#/properties/strukturertadresse/properties/gateadresse/properties/husbokstav",
              "type": [
                "string",
                "null"
              ],
              "title": "The Husbokstav Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "husnummer": {
              "$id": "#/properties/strukturertadresse/properties/gateadresse/properties/husnummer",
              "type": [
                "integer",
                "null"
              ],
              "title": "The Husnummer Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "gatenavn": {
              "$id": "#/properties/strukturertadresse/properties/gateadresse/properties/gatenavn",
              "type": [
                "string",
                "null"
              ],
              "title": "The Gatenavn Schema",
              "default": "",
              "examples": [
                "Olav Kyrres gate 9"
              ],
              "pattern": "^(.*)$"
            }
          }
        },
        "postnr": {
          "$id": "#/properties/strukturertadresse/properties/postnr",
          "type": [
            "string",
            "null"
          ],
          "title": "The Postnr Schema",
          "default": "",
          "examples": [
            "3936"
          ],
          "pattern": "^(.*)$"
        },
        "poststed": {
          "$id": "#/properties/strukturertadresse/properties/poststed",
          "type": [
            "string",
            "null"
          ],
          "title": "The Poststed Schema",
          "default": "",
          "examples": [
            "PORSGRUNN"
          ],
          "pattern": "^(.*)$"
        },
        "land": {
          "$id": "#/properties/strukturertadresse/properties/land",
          "type": [
            "string",
            "null"
          ],
          "title": "The Land Schema",
          "default": "",
          "examples": [
            "NORGE"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}
