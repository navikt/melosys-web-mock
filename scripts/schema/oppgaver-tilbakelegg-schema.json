{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/oppgaver-tilbakelegg-schema.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "oppgaveID",
    "begrunnelse",
    "venterPaaDokumentasjon"
  ],
  "properties": {
    "oppgaveID": {
      "$id": "#/properties/oppgaveID",
      "type": "string",
      "title": "The oppgaveID Schema",
      "default": "",
      "examples": [
        "1000023948"
      ],
      "pattern": "^(.*)$"
    },
    "begrunnelse": {
      "$id": "#/properties/begrunnelse",
      "type": ["string", "null"],
      "title": "The begrunnelse Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "venterPaaDokumentasjon": {
      "$id": "#/items/properties/venterPaaDokumentasjon",
      "type": "boolean",
      "title": "The venterPaaDokumentasjon Schema",
      "default": false,
      "examples": [
        true
      ]
    }
  }
}
