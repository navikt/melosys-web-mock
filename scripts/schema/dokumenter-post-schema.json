{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/dokumenter-post-schema.json",
  "type": [
    "object",
    "null"
  ],
  "title": "The Root Schema",
  "additionalProperties": false,
  "required": [
    "mottaker",
    "begrunnelseKoder",
    "fritekst"
  ],
  "properties": {
    "mottaker": {
      "$id": "#/properties/mottaker",
      "type": [
        "string",
        "null"
      ],
      "title": "The Mottaker Schema",
      "description": "",
      "default": "",
      "examples": [
        "ARBEIDSGIVER",
        "BRUKER",
        "REPRESENTANT",
        "MYNDIGHET"
      ],
      "enum": ["ARBEIDSGIVER","BRUKER","REPRESENTANT","MYNDIGHET"],
      "pattern": "^(.*)$"
    },
    "fritekst": {
      "$id": "#/properties/fritekst",
      "type": [
        "string",
        "null"
      ],
      "title": "The Fritekst Schema",
      "default": "",
      "examples": ["Fritekst"],
      "pattern": "^(.*)$"
    },
    "begrunnelseKoder": {
      "$id": "#/properties/begrunnelseKoder",
      "type": "array",
      "title": "The BegrunnelseKode Schema",
      "default": "",
      "examples": [
        "[IKKE_VESENTLIG_VIRKSOMHET]"
      ],
      "items": {
        "$id": "#/properties/begrunnelseKoder/items",
        "type": "string",
        "uniqueItems": true,
        "title": "The Items Schema",
        "additionalProperties": false
      }
    }
  }
}
