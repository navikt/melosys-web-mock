{
  "definitions": {
    "nullable-dokument":  {
      "type": "object",
      "title": "Nullable Dokument type definisjon",
      "additionalProperties": false,
      "required": [
        "dokumentID",
        "tittel",
        "mottattDato"
      ],
      "properties": {
        "dokumentID": {
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/nullable-dokumentID"
        },
        "tittel": {
          "type": "string",
          "title": "Tittel Schema",
          "examples": ["Tittelen her"]
        },
        "mottattDato": {
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/nullable-dato-tid"
        }
      }
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/dokumenter-oversikt-schema.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "mottaksretning",
      "addressat",
      "journalpostID",
      "hoveddokument",
      "vedlegg"
    ],
    "properties": {
      "mottaksretning": {
        "$id": "#/items/properties/mottaksretning",
        "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/kodeverk"
      },
      "addressat": {
        "$id": "#/items/properties/addressat",
        "type": "string",
        "title": "The Addressat Schema",
        "default": "",
        "examples": [
          "Ole Hermanrud"
        ],
        "pattern": "^(.*)$"
      },
      "journalpostID": {
        "$id": "/properties/journalpostID",
        "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/journalpostID"
      },
      "hoveddokument": {
        "$id": "#/items/properties/hoveddokument",
        "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dokument"
      },
      "vedlegg": {
        "$id": "#/items/properties/vedlegg",
        "type": "array",
        "title": "The Vedlegg Schema",
        "items": {
          "$id": "#/items/properties/vedlegg/items",
          "$ref": "#/definitions/nullable-dokument"
        }
      }
    }
  }
}
