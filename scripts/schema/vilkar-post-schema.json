{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://nav.no/vilkar-post.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "oppfylt",
    "begrunnelseKoder",
    "begrunnelseFritekst"
  ],
  "properties": {
    "oppfylt": {
      "$id": "#/properties/oppfylt",
      "type": "boolean",
      "title": "The Oppfylt Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "begrunnelseKoder": {
      "$id": "#/properties/begrunnelseKoder",
      "type": "array",
      "title": "The Begrunnelsekoder Schema",
      "items": {
        "$id": "#/properties/begrunnelseKoder/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "BEGRUNNELSE"
        ],
        "pattern": "^(.*)$"
      }
    },
    "begrunnelseFritekst": {
      "$id": "#/properties/begrunnelseFritekst",
      "type": "string",
      "title": "The Begrunnelsefritekst Schema",
      "default": "",
      "examples": [
        "En begrunnelsestekst"
      ],
      "pattern": "^(.*)$"
    }
  }
}
