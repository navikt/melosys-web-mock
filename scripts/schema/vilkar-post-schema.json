{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://nav.no/vilkar-post.json",
  "type": "array",
  "title": "The Root Schema",
  "minItems": 0,
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "additionalProperties": false,
    "required": [
      "oppfylt",
      "begrunnelseKoder",
      "begrunnelseFritekst"
    ],
    "properties": {
      "oppfylt": {
        "$id": "#/items/properties/oppfylt",
        "type": "boolean",
        "title": "The Oppfylt Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "begrunnelseKoder": {
        "$id": "#/items/properties/begrunnelseKoder",
        "type": "array",
        "title": "The Begrunnelsekoder Schema",
        "minItems": 1,
        "items": {
          "$id": "#/items/properties/begrunnelseKoder/items",
          "type": "string",
          "title": "The Items Schema",
          "default": "",
          "examples": [
            "BEGRUNNELSE"
          ],
          "pattern": "^(.*)$"
        }
      },
      "begrunnelseFritekst": {
        "$id": "#/items/properties/begrunnelseFritekst",
        "type": "string",
        "title": "The Begrunnelsefritekst Schema",
        "default": "",
        "examples": [
          "En begrunnelsestekst"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}
